{
  "name": "xccup-client",
  "version": "0.2.0",
  "private": true,
  "license": "GNU GPLv3",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "build-local": "vue-tsc --noEmit && vite build --mode staging",
    "serve": "vite preview --host",
    "preview": "vue-tsc --noEmit && vite build --mode staging && vite preview --host",
    "preview-8000": "vue-tsc --noEmit && vite build --mode staging && vite preview --host --port 8000",
    "cypress-gui": "cypress open",
    "cypress-headless": "cypress run",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier .  --write",
    "up": "npx taze -I"
  },
  "dependencies": {
    "@sentry/tracing": "^7.120.3",
    "@sentry/vue": "^8.55.0",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vueuse/core": "^12.7.0",
    "@vueuse/integrations": "^12.7.0",
    "axios": "^1.8.1",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^4.4.8",
    "chartjs-adapter-luxon": "^1.3.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "glightbox": "^3.3.1",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "leaflet-gesture-handling": "^1.2.2",
    "lodash-es": "^4.17.21",
    "luxon": "^3.5.0",
    "marked": "^15.0.7",
    "metar-parser": "^1.0.1",
    "nprogress": "^0.2.0",
    "sweetalert2": "^11.17.2",
    "v-plausible": "^1.2.0",
    "vue": "^3.5.13",
    "vue-avatar-editor-improved": "^1.0.7",
    "vue-demi": "^0.14.10",
    "vue-router": "^4.5.0",
    "vue3-datepicker": "^0.4.0",
    "vue3-emoji-picker": "^1.1.8"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.23.0",
    "@types/bootstrap": "^5.2.10",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.16",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.23",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.17.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-terminal-report": "^7.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.6.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.85.1",
    "typescript": "^5.8.2",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^5.4.14",
    "vue-tsc": "^2.2.8"
  },
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "linux",
        "current"
