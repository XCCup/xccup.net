    "bootstrap": "5.3.0",
    "bootstrap-icons": "^1.10.5",
    "chart.js": "^4.3.0",
    "chartjs-adapter-luxon": "^1.3.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "glightbox": "^3.2.0",
    "jwt-decode": "^3.1.2",
    "leaflet": "^1.9.4",
    "leaflet-gesture-handling": "^1.2.2",
    "lodash-es": "^4.17.21",
    "luxon": "^3.3.0",
    "marked": "^5.0.4",
    "metar-parser": "^1.0.1",
    "nprogress": "^0.2.0",
    "sweetalert2": "^11.7.10",
