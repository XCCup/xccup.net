  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.4.0",
    "@types/bootstrap": "^5.2.6",
    "@types/geojson": "^7946.0.10",
    "@types/leaflet": "^1.9.3",
    "@types/lodash-es": "^4.17.8",
    "@types/marked": "^5.0.1",
    "@types/node": "^20.4.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
