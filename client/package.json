{
  "name": "xccup-client",
  "version": "0.2.0",
  "private": true,
  "license": "GNU GPLv3",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "build-local": "vue-tsc --noEmit && vite build --mode staging",
    "serve": "vite preview --host",
    "preview": "vue-tsc --noEmit && vite build --mode staging && vite preview --host",
    "preview-8000": "vue-tsc --noEmit && vite build --mode staging && vite preview --host --port 8000",
    "cypress-gui": "cypress open",
    "cypress-headless": "cypress run",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier .  --write",
    "up": "taze -I"
  },
  "dependencies": {
    "@sentry/tracing": "^7.84.0",
    "@sentry/vue": "^7.84.0",
    "@vueuse/integrations": "^10.6.1",
    "axios": "1.6.2",
    "bootstrap": "5.3.2",
    "bootstrap-icons": "^1.11.2",
    "chart.js": "^4.4.0",
    "chartjs-adapter-luxon": "^1.3.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "glightbox": "^3.2.0",
    "jwt-decode": "3.1.2",
    "leaflet": "^1.9.4",
    "leaflet-gesture-handling": "^1.2.2",
    "lodash-es": "^4.17.21",
    "luxon": "^3.4.4",
    "marked": "^11.0.0",
    "metar-parser": "^1.0.1",
    "nprogress": "^0.2.0",
    "sweetalert2": "^11.10.1",
    "vue": "^3.3.9",
    "vue-avatar-editor-improved": "^1.0.7",
